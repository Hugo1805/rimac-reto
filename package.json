{
  "name": "rimac-reto-serverless",
  "version": "1.0.0",
  "description": "API RESTful con Serverless Framework para el reto técnico de Rimac",
  "main": "handler.js",
  "scripts": {
    "build": "tsc",
    "deploy": "./scripts/deploy.sh",
    "deploy:dev": "./scripts/deploy.sh dev",
    "deploy:prod": "./scripts/deploy.sh prod",
    "remove": "serverless remove",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "offline": "serverless offline",
    "dev": "./scripts/dev.sh",
    "setup": "./scripts/setup.sh",
    "generate-token": "node scripts/generateToken.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "dynamodb:install": "serverless dynamodb install",
    "dynamodb:start": "serverless dynamodb start"
  },
  "dependencies": {
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1691.0",
    "axios": "^1.7.7",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.145",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.16.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "serverless": "^3.39.0",
    "serverless-dynamodb-local": "^0.2.40",
    "serverless-offline": "^13.6.0",
    "serverless-plugin-typescript": "^2.1.5",
    "ts-jest": "^29.2.4",
    "typescript": "^5.5.4"
  },
  "keywords": [
    "serverless",
    "aws",
    "lambda",
    "dynamodb",
    "typescript",
    "api",
    "rest"
  ],
  "author": "Hugo Ponce de León",
  "license": "MIT"
}
